{
	"extends": "@repo/typescript-config/nextjs.json",
	"compilerOptions": {
		"plugins": [
			{
				"name": "next"
			}
		],
		"noEmit": true
	},
	"include": [
		"next-env.d.ts",
		"next.config.mjs",
