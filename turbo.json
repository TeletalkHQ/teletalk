{
	"globalEnv": [
		"NODE_ENV",
		"NEXT_PUBLIC_RUNTIME_MODE",
		"NEXT_PUBLIC_CLIENT_BASE_URL",
		"NEXT_PUBLIC_SERVER_BASE_URL"
	],
	"$schema": "https://turbo.build/schema.json",
	"globalDependencies": [".env"],
	"tasks": {
		"build": {
			"env": [".env"],
			"outputs": [".next/**", "!.next/cache/**"]
		},
		"check:lint": {},
		"check:lint:fix": {},
		"check:lint:fix:strict": {},
		"check:style": {},
		"check:types": {
			"cache": false
		},
		"check:types:watch": {
			"cache": false,
			"persistent": true
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"start": {
			"cache": false,
			"persistent": true
		},
		"start:dev": {
			"cache": false,
			"persistent": true
		},
		"nest:start:dev": {
			"cache": false,
			"persistent": true
		},
		"test": {},
		"test:coverage": {},
		"test:dev": {},
		"test:dev:coverage": {},
		"test:dev:watch": {
			"cache": false,
			"persistent": true
		}
	}
}
